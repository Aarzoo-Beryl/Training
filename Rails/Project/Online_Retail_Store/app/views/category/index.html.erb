<div class="Category Index">
  <h1 align="center">CATEGORY</h1><hr>
  <%# <%= link_to("Add New Address",new_category_path,:class=>'action new')%>
  <br><br>
  <table class="heading" style="border: 3px solid black;" border="1" cellpadding=13 cellspacing=3 align='center'>
    <tr>
      <th>ID</th>
      <th>TITLE</th>
      <th>DESCRIPTION</th>
      <th>CREATED AT</th>
      <th>UPDATED AT</th>
      <th>ACTIONS</th>
    </tr>
    <% @categories.each do |category|%>
    <tr>
      <td><%= category.id %></td>
      <td><%= category.title %></td>
      <td><%= category.description %></td>
      <td><%= category.created_at %></td>
      <td><%= category.updated_at %></td>
      <td>
        <%# <%= link_to("Show",category_path(category),:class=>"action show") %>
        <%# <%= link_to("Edit",edit_category_path(category),:class=>"action edit")%>
        <%# <%= link_to("Delete",delete_category_path(category),:class=>"action delete")%>
      </td>
    </tr>
    <% end %>
  </table>
</div>
